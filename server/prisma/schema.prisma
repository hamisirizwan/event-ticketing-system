generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id @default(autoincrement())
  name     String  @db.VarChar(255)
  is_admin Boolean @default(false)
  phone    String  @unique @db.VarChar(12)
  email    String  @unique @db.VarChar(255)
  password String
}
